{
  "name": "electron-music-player",
  "version": "1.0.0",
  "description": "electron-music-player",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "build"
  },
  "build": {
    "appId": "electronMusicPlayer",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "dmg": {
      "background": "build/appdmg.png",
      "icon": "build/icon.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 380,
          "y": 280,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 110,
          "y": 280,
          "type": "file"
        }
      ],
      "window": {
        "width": 500,
        "height": 500
      }
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "keywords": [
    "electron",
    "music-player"
  ],
  "author": "songzeng",
  "license": "ISC",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^20.44.4"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "electron-store": "^3.3.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "uuid": "^3.3.2"
  }
}
